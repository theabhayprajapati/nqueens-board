{
  "plugins": ["functional"],
  "extends": ["plugin:functional/recommended"],
  "rules": {
    "functional/immutable-data": "error"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "ignorePatterns": ["node_modules/", "main.js"]
}
